<!DOCTYPE html>
<html>
  <head>
    <title>NflSuicideLeague</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
  </head>

  <body>
    <% if current_user %>
      <%= link_to "Log Out", log_out_path %>
    <% else %>
      <%= link_to "Log In", log_in_path %>
    <% end %>
    <%= yield %>
  </body>
</html>
